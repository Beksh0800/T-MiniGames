{
  "name": "telegram-mini-games",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "client/telegram-mini-games/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
